{
    "project": {
        "break": "C",
        "builders": "*.com,*.mms,*.opt",
        "exclude": "**/{.vscode}/**,**/.git/**,*gitignore*,*.zip,*.lis,*.map,*.obj,*.exe,**/Mac/**,**/PC/**,**/PCbuild/**",
        "headers": "*.h,*.inc",
        "listing": "*.lis,*.map",
        "outdir": "out",
        "projectName": "Python",
        "projectType": "executable",
        "resource": "**/Doc/**,**/Grammar/**,**/Include/**,**/Lib/**,**/m4/**,**/Modules/**,**/Objects/**,**/Parser/**,**/Programs/**,**/Python/**,**/Tools/**,*.py",
        "root": "Python_3_8_1",
        "source": "*.c",
        "addLibraries": "",
        "addIncludes": ""
    },
    "synchronize": {
        "downloadNewFiles": "skip",
        "keepAlive": false,
        "preferZip": true,
        "forceLocalTime": true,
        "purge": true,
        "setTimeAttempts": 3
    },
    "builds": {
        "configurations": [
            {
                "label": "DEBUG",
                "description": "DEBUG",
                "command": "build_mms.com",
                "parameter": "DEBUG"
            },
            {
                "label": "RELEASE",
                "description": "RELEASE",
                "command": "build_mms.com",
                "parameter": "RELEASE"
            }
        ]
    }
}