{
    "project": {
        "break": "C",
        "builders": "*.com,*.mms,*.opt",
        "exclude": "**/{.vscode}/**,**/.git/**,*gitignore*,*.lis,*.map,*.obj,*.exe,*.o,*.so,*.gcda,*.pyc,**/Mac/**,**/PC/**,**/PCbuild/**,Python3.zip,lists.zip,defs.zip",
        "headers": "*.h,*.inc",
        "listing": "*.lis,*.map",
        "outdir": "out",
        "projectName": "Python3",
        "projectType": "executable",
        "resource": "**/{Doc,Grammar,Include,Lib,m4,Modules,Objects,Parser,Programs,Python,Tools,vms}/**,*.py,*.pcsi*",
        "root": "/wrk_disk/vorfolomeev/Python3",
        "source": "*.c",
        "addCompQual": "",
        "addCompDef": "",
        "addLibraries": "",
        "addIncludes": ""
    },
    "synchronize": {
        "downloadNewFiles": "skip",
        "keepAlive": false,
        "preferZip": true,
        "forceLocalTime": true,
        "purge": true,
        "setTimeAttempts": 3
    },
    "builds": {
        "configurations": [
            {
                "label": "DEBUG",
                "description": "Python 3.8.2 debug",
                "command": "Python3.mms",
                "parameter": "/MACRO=(\"OUTDIR=OUT\",\"CONFIG=DEBUG\")"
            },
            {
                "label": "RELEASE",
                "description": "Python 3.8.2 release",
                "command": "Python3.mms",
                "parameter": "/MACRO=(\"OUTDIR=OUT\",\"CONFIG=RELEASE\")"
            }
        ]
    }
}